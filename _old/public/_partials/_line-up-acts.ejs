<h2>Bands</h2>
<ul class="acts list-unstyled">
  <%
    for (var i=0; i<public["line-up"]._data.acts.length; i++) {
      var actID = public["line-up"]._data.acts[i];
      var act = public["line-up"]._data[actID];
  %>
  
    <li>
      <a href="./<%= act.slug %>">
        <img class="bandpic img-responsive" src="<%= act.image %>" />
        <h3><%= act.name %></h3>
      </a>
      <p class="banddesc">
        <%= act.description %>
      </p>
    </li>
  <%
    }
  %>
  
</ul>

