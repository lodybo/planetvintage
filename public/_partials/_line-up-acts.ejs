<h2>Bands</h2>
<ul class="acts list-unstyled">
  <%
    for (var act in public["line-up"]._data) {
    act = public["line-up"]._data[act];
  %>
    <li>
      <a href="./<%= act.slug %>">
        <img class="bandpic img-responsive" src="<%= act.image %>" />
        <h3><%= act.name %></h3>
      </a>
      <p class="banddesc">
        <%= act.description %>
      </p>
    </li>
  <%
    }
  %>
  
</ul>

